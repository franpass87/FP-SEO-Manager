{
    "name": "fp/fp-seo-performance",
    "description": "Provides an on-page SEO analyzer with configurable checks, bulk audits, and admin guidance for editors.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "FP\\SEO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FP\\SEO\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "brain/monkey": "^2.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "wp-coding-standards/wpcs": "^3.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "php-stubs/wordpress-stubs": "^6.2",
        "rector/rector": "^0.17.0",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": [
            "@phpcs"
        ],
        "phpcs": "vendor/bin/phpcs",
        "phpstan": "vendor/bin/phpstan analyse",
        "rector": "vendor/bin/rector process",
        "sync:author": "node tools/sync-author-metadata.js --apply=${APPLY:-false}",
        "sync:docs": "node tools/sync-author-metadata.js --docs --apply=${APPLY:-false}",
        "changelog:from-git": "conventional-changelog -p angular -i CHANGELOG.md -s || true"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "8.2.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "homepage": "https://francescopasseri.com",
    "support": {
        "issues": "https://francescopasseri.com/contact/"
    },
    "authors": [
        {
            "name": "Francesco Passeri",
            "email": "info@francescopasseri.com",
            "homepage": "https://francescopasseri.com",
            "role": "Developer"
        }
    ]
}
