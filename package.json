{
  "name": "fp-seo-performance",
  "version": "0.1.2",
  "description": "Provides an on-page SEO analyzer with configurable checks, bulk audits, and admin guidance for editors.",
  "author": "Francesco Passeri <info@francescopasseri.com> (https://francescopasseri.com)",
  "license": "GPL-2.0-or-later",
  "homepage": "https://francescopasseri.com",
  "bugs": {
    "url": "https://francescopasseri.com/contact/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/franpass87/FP-SEO-Manager.git"
  },
  "scripts": {
    "sync:author": "node tools/sync-author-metadata.js --apply=${APPLY:-false}",
    "sync:docs": "node tools/sync-author-metadata.js --docs --apply=${APPLY:-false}",
    "changelog:from-git": "conventional-changelog -p angular -i CHANGELOG.md -s || true",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "test:js:coverage": "jest --coverage"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^3.0.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/assets/**/*.test.js",
      "**/tests/js/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "assets/admin/js/**/*.js",
      "!assets/admin/js/**/*.test.js",
      "!assets/admin/js/**/index.js"
    ],
    "coverageDirectory": "coverage/js",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
